generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Application {
  id          Int      @id @default(autoincrement())
  position    String
  company     String
  dateApplied DateTime?
  status      String
  notes       String?
  url         String?
  createdAt   DateTime @default(now())
}

